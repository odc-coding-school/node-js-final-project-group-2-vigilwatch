 <nav id="main-navbar" class="navbar navbar-expand-lg navbar-light bg-white fixed-top shadow-sm">
  <!-- Container wrapper -->
  <div class="container-fluid">
      <!-- Toggle button -->
      
 
      <!-- Brand -->
      <a class="navbar-brand" href="#">
          <img src="/images/Logo.png" height="45"  alt="logo" loading="lazy" />
      </a>
 
      <!-- Right links -->
      <ul class="navbar-nav ms-auto d-flex flex-row">
          <li class="nav-item">
              <a class="nav-link me-3 me-lg-0" href="/notification" id="notification" role="button">
                  <i class="fas fa-bell" style="font-size: 1.2rem;margin-top: 10px;"></i>
                  <span class="badge rounded-pill badge-notification bg-danger">0</span>
