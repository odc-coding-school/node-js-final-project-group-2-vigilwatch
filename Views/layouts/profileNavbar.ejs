                <span class="badge rounded-pill badge-notification bg-danger">0</span>
              </a>
            </li>
            <li class="nav-item me-3 me-lg-0" style="margin-top: 5px;">
              <a class="nav-link" href="#" id="theme-toggle" onclick="toggleTheme()">
                <i class="fas fa-moon" id="theme-icon" style="margin-top: 6px;"></i>
              </a>
            </li>
            <li class="nav-item dropdown">
              <a class="nav-link d-flex align-items-center" href="#" id="profile" role="button" aria-expanded="false">
                  <img src="<%= userProfile %>" class="rounded-circle" height="40" width="40" loading="lazy" alt="user"
                       style="box-shadow: inset 0 0 52px rgba(0, 0, 0, 0.5);" />
                  &nbsp;
              </a>
          </li>
 
         
          
      </ul>
  </div>
  <!-- Container wrapper -->
   
 </nav>
 
 
